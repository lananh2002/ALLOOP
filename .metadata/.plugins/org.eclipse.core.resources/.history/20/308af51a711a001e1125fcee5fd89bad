package Bai05;

import java.util.Date;

public class LopThucPham {
	private String maHang; 
	private String tenHang; 
	private int donGia; 
	Date ngaySX ;
	Date ngayHH;
	
	
	
	public String getMaHang() {
		return maHang;
	}
	public void setMaHang(String maHang) {
		if (maHang.trim().equals("")) {
			this.maHang = maHang;
		} else {
			System.out.println("Loi : Khong duoc bo trong");
		}

	}
	public String getTenHang() {
		return tenHang;
	}
	
	public void setTenHang(String tenHang) {
		if (tenHang.trim().equals("")){
			this.tenHang = tenHang;
		} else {
			System.out.println("Loi : Khong bo trong");
		}
	
	}
	public int getDonGia() {
		return donGia;
	}
	public void setDonGia(int donGia) {
		if (donGia > 0) {
			this.donGia = donGia;
		} else {
			System.out.println("Loi : Lon hon 0");
		}

	}
	public Date getNgaySX() {
		return ngaySX;
	}
	public void setNgaySX(Date ngaySX) {
		
		this.ngaySX = ngaySX;
	}
	public Date getNgayHH() {
		return ngayHH;
	}
	public void setNgayHH(Date ngayHH) {
		this.ngayHH = ngayHH;
	}
	public LopThucPham(String maHang, String tenHang, int donGia, Date ngaySX, Date ngayHH) {
		super();
		this.maHang = maHang;
		this.tenHang = tenHang;
		this.donGia = donGia;
		this.ngaySX = ngaySX;
		this.ngayHH = ngayHH;
	}
	public LopThucPham() {
		super();
		// TODO Auto-generated constructor stub
	}

	
	
	

}
